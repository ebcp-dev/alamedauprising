<!--Carousel Wrapper-->
    <div id="home" class="carousel slide carousel-fade white-text" data-ride="carousel" data-interval="false">
        <!--Indicators-->
        <!--/.Indicators-->

        <!--Slides-->
        <div class="carousel-inner" role="listbox">

            <!-- First slide -->
            <div class="carousel-item active view hm-black-light" style="background-image: url('images/coketourny3.jpg'); background-repeat: no-repeat; background-size: cover;">
                
                <!-- Caption -->
                <div class="full-bg-img flex-center white-text">
                    <ul class="animated fadeIn col-md-12">
                        <li>
                            <p>We Are</p>
                        </li>
                        <li class="pinktext">
                            <h1 class="h1-responsive">Alameda Uprising</h1></li>
                        </li>
                        <li>
                            <p>Alameda, California</p>
                        </li>
                    </ul>
                </div>
                <!-- /.Caption -->
                
            </div>
            <!--/.First slide-->

            <!-- Second slide -->
            <div class="carousel-item view hm-black-light" style="background-image: url('images/coketourny2.jpg'); background-repeat: no-repeat; background-size: cover;">
                
                <!-- Caption -->
                <div class="full-bg-img flex-center white-text">
                    <ul class="animated fadeIn col-md-12">
                        <li class="pinktext">
                            <a href="#contact" 
                            class="btn btn-info btn-lg" rel="nofollow">
                            Follow us on social media!
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- /.Caption -->
                
            </div>
            <!--/.Second slide -->

            <!-- Third slide -->
            <div class="carousel-item view hm-black-light" style="background-image: url('images/coketourny7.jpg'); background-repeat: no-repeat; background-size: cover;">
                
                <!-- Caption -->
                <div class="full-bg-img flex-center white-text">
                    <ul class="animated fadeIn col-md-12">
                        <li class="pinktext">
                            <a href="#schedule"
                            class="btn btn-info btn-lg" rel="nofollow">
                            Come see our next game!
                            </a>
                        </li>
                    </ul>
                </div>
                <!-- /.Caption -->
                
            </div>
            <!--/.Third slide-->
        </div>
        <!--/.Slides-->

        <!--Controls-->
        <a class="carousel-control-prev" href="#home" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#home" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
        <!--/.Controls-->
    </div>
    <!--/.Carousel Wrapper-->

<br>

<!--Content-->
    <div class="container">
        <div class="row">
            <!--First columnn-->
            <div class="col-lg-4">
                <!--Card-->
                <div class="card wow fadeIn" data-wow-delay="0.2s">

                    <!--Card image-->
                    <div class="view overlay hm-white-slight">
                        <img src="images/coketourny4.jpg" class="img-fluid" alt="">
                        <a href="#!">
                            <div class="mask waves-effect waves-light"></div>
                        </a>
                    </div>
                    <!--/.Card image-->

                    <!--Card content-->
                    <div class="card-block" id="schedule">
                        <!--Title-->
                        <h4 class="card-title">Schedule</h4>
                        <!--Text-->
                        <p class="card-text">Upcoming games.</p>
                        <a href="#gameschedule" 
                        class="btn btn-info" data-toggle="collapse"
                        aria-expanded="false" aria-controls="gameschedule"
                        >
                        See Schedule
                        </a>
                        <div class="collapse" id="gameschedule">
                            <div class="card-block">
                                TBA
                            </div>
                        </div>
                    </div>
                    <!--/.Card content-->

                </div>
                <!--/.Card-->
            </div>
            <!--First columnn-->

            <!--Second columnn-->
            <div class="col-lg-4">
                <!--Card-->
                <div class="card wow fadeIn" data-wow-delay="0.4s">

                    <!--Card image-->
                    <div class="view overlay hm-white-slight">
                        <img src="images/coketourny5.jpg" class="img-fluid" alt="">
                        <a href="#!">
                            <div class="mask waves-effect waves-light"></div>
                        </a>
                    </div>
                    <!--/.Card image-->

                    <!--Card content-->
                    <div class="card-block" id="roster">
                        <!--Title-->
                        <h4 class="card-title">Roster</h4>
                        <!--Text-->
                        <p class="card-text">Meet our players.</p>
                        <a href="#rosterlist" 
                        class="btn btn-info" data-toggle="collapse"
                        aria-expanded="false" aria-controls="rosterlist"
                        >
                        See Roster
                        </a>
                        <div class="collapse" id="rosterlist">
                            <div class="card-block texter-center">
                                <h3 class="h3-responsive text-center">PLAYERS</h3>
                                <ul class="list-group">
                                    {{#each roster}}
                                    <li class="list-group-item justify-content-between">
                                        {{name}}
                                        <span class="badge badge-primary badge-pill">{{jersey}}</span>
                                    </li>
                                    {{/each}}
                                </ul>
                                <br>
                                <h3 class="h3-responsive text-center">HEAD COACH</h3>
                                <ul class="list-group">
                                    <li class="list-group-item justify-content-between">
                                        {{headcoach}}
                                    </li>
                                </ul>
                                <p></p>
                                <br>
                                <h3 class="h3-responsive text-center">ASSISTANT COACHES</h3>
                                <ul class="list-group">
                                    {{#each assistantcoaches}}
                                    <li class="list-group-item justify-content-between">
                                        {{this}}
                                    </li>
                                    {{/each}}
                                </ul>
                                <br>
                                <h3 class="h3-responsive text-center">GENERAL MANAGER</h3>
                                <ul class="list-group">
                                    <li class="list-group-item justify-content-between">
                                        {{generalmanager}}
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <!--/.Card content-->

                </div>
                <!--/.Card-->
            </div>
            <!--Second columnn-->

            <!--Third columnn-->
            <div class="col-lg-4">
                <!--Card-->
                <div class="card wow fadeIn" data-wow-delay="0.6s">

                    <!--Card image-->
                    <div class="view overlay hm-white-slight">
                        <img src="images/coketourny1.jpg" class="img-fluid" alt="">
                        <a href="#!">
                            <div class="mask waves-effect waves-light"></div>
                        </a>
                    </div>
                    <!--/.Card image-->

                    <!--Card content-->
                    <div class="card-block" id="gallery">
                        <!--Title-->
                        <h4 class="card-title">Gallery</h4>
                        <!--Text-->
                        <p class="card-text">Team pictures.</p>
                        <a href="#pictures" 
                        class="btn btn-info" data-toggle="collapse"
                        aria-expanded="false" aria-controls="pictures"
                        >
                        See Gallery
                        </a>
                        <div class="collapse" id="pictures">
                            {{#each pics}}
                            <div class="container">
                            <div class="row">
                                <div class="card">
                                    <img class="img-fluid" src="images/{{this}}.jpg">
                                </div>
                                <br>
                            </div>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                    <!--/.Card content-->

                </div>
                <!--/.Card-->
            </div>
            <!--Third columnn-->
        </div>
    </div>
    <!--/.Content-->



    